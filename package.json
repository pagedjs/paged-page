{
  "name": "@pagedjs/paged-page",
  "version": "0.0.1b",
  "description": "paged-page is a webcomponent that shows on screen a paginated preview of the HTML inside of it",
  "main": "paged-page",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve"
  },
  "dependencies": {
    "lit": "^3.1.0",
    "pagedjs": "^0.4.3"
  },
  "author": " Fred Chasen, Gijs de Heij, julientaq",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^14.2.5"
  }
}
